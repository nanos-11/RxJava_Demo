<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/tv_teach"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:text="给初学者的RxJava2.0教程(五)" />


    <Button
        android:id="@+id/rxJava01"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="回顾zip发送无数个事件" />

    <Button
        android:id="@+id/rxJava02"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="测试zip同步和异步内存消耗" />

    <Button
        android:id="@+id/rxJava03"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="filter 操作符" />

    <Button
        android:id="@+id/rxJava04"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="sample 操作符" />

    <Button
        android:id="@+id/rxJava05"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="减慢发送事件的速度" />


    <Button
        android:id="@+id/rxJava08"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="给初学者的RxJava2.0教程(六)" />

</LinearLayout>